{
  "name": "gertvdb/date_interval",
  "type": "drupal-module",
  "description": "Provide a standard way to store a date interval in Drupal",
  "keywords": ["date_interval"],
  "homepage": "https://github.com/gertvdb/date_interval",
  "license": "MIT",
  "authors": [
    {
      "name": "Gert van den Buijs",
      "email": "gertvandenbuijs@hotmail.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "autoload": {
    "psr-4": {
      "Drupal\\date_interval\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drupal\\Tests\\date_interval\\": "tests/src/",
      "Drupal\\Tests\\": "vendor/drupal/core/tests/Drupal/Tests"
    }
  },
  "require-dev": {
    "drupal/coder": "^8.2",
    "drupal/core": "^8.2",
    "jakub-onderka/php-console-highlighter": "^0.3.2",
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "phpmd/phpmd": "^2.6",
    "phpunit/phpunit": "^4 || ^5.7"
  }
}
